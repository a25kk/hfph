version: '3'

services:
  web:
    networks:
      - default
      - traefik_gateway
    labels:
      - traefik.enable=true
      - traefik.docker.network=traefik_gateway
      - traefik.frontend.rule=HostRegexp:web.${site:sitename}.{domain},{something:[a-z]+}.web.${site:sitename}.{domain}
networks:
  default:
  traefik_gateway:
    external: true