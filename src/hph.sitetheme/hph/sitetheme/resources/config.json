{
  "name": "hfph",
  "paths": {
    "app": "app/",
    "base": "../",
    "dev": "_site/",
    "dist": "dist/",
    "diazoPrefix": "/++theme++hfph.sitetheme/",
    "src": "node_modules/"
  },
  "favicon": {
    "dataFile": "faviconData.json",
    "masterPicture": "favicon.png",
    "iconPath": "assets/ico/",
    "ios": "#ffffff",
    "windows": "#2d89ef",
    "androidBackground": "#ffffff",
    "androidColor": "#ffffff",
    "safariColor": "#5bbad5",
    "revisionKey": "iet7OjeChoz1",
    "html": "_includes/base/head.html",
    "htmlDist": "_includes/base/"
  },
  "scripts": {
    "app": [
        "scripts/a25.js",
        "scripts/a25.helpers.js",
        "scripts/a25.slider.js",
        "scripts/a25.navbar.js",
        "scripts/app.js",
        "scripts/dropdown.js",
        "scripts/eventbox.js",
        "scripts/interdependentselect.js",
        "scripts/navbar.js",
        "scripts/slider.js",
        "scripts/utils.js"
        ],
    "src": [
        "dropmic/dist/dropmic.js",
        "flickity/dist/flickity.pkgd.js",
        "fontfaceobserver/fontfaceobserver.js",
        "lazysizes/lazysizes-umd.js",
        "lazysizes/plugins/parent-fit/ls.parent-fit.js",
        "respimage/respimage.js",
        "svg4everybody/dist/svg4everybody.js",
        "vh-check/dist/vh-check.min.js",
        "x-ray/dist/js/x-ray.js"
    ],
    "base": [
        "dropmic/dist/dropmic.js",
        "flickity/dist/flickity.pkgd.js",
        "fontfaceobserver/fontfaceobserver.js",
        "hideShowPassword/hideShowPassword.js",
        "jquery/dist/jquery.js",
        "JVFloat/jvfloat.js",
        "lazysizes/lazysizes.js",
        "lazysizes/plugins/ls.parent-fit.js",
        "mailcheck/src/mailcheck.js",
        "requirejs/require.js",
        "respimage/respimage.js",
        "x-ray/dist/js/x-ray.js"
    ]
  },
  "styles": {
      "src": [
          "rfs/scss/_rfs.scss",
          "sass-mq/_mq.scss"
      ]
  },
  "replacementPatterns": {
        "base": [
            {
                "match": "/../dist/styles/",
                "replacement": "/styles/"
            },
            {
                "match": "/dist/styles/",
                "replacement": "/styles/"
            }
        ],
        "server": [
            {
                "match": "../../assets/",
                "replacement": "../assets/"
            },
            {
                "match": "../../dist/styles/",
                "replacement": "/styles/"
            },
            {
                "match": "../dist/styles/",
                "replacement": "/styles/"
            },
            {
                "match": "../../dist/scripts/",
                "replacement": "/scripts/"
            },
            {
                "match": "../dist/scripts/",
                "replacement": "/scripts/"
            }
        ],
        "pat": [
            {
                "match": "../assets/",
                "replacement": "assets/"
            },
            {
                "match": "assets/",
                "replacement": "/++theme++hfph.sitetheme//dist/assets/"
            },
            {
                "match": "../styles/",
                "replacement": "/++theme++hfph.sitetheme/dist/styles/"
            },
            {
                "match": "styles/",
                "replacement": "/++theme++hfph.sitetheme/dist/styles/"
            },
            {
                "match": "/styles/",
                "replacement": "/++theme++hfph.sitetheme/dist/styles/"
            },
            {
                "match": "../scripts/",
                "replacement": "/++theme++hfph.sitetheme/scripts/"
            },
            {
                "match": "scripts/",
                "replacement": "/++theme++hfph.sitetheme/scripts/"
            }
        ]
    },
    "sprite": {
        "mode": {
            "inline": true,
            "symbol": {
                "prefix": "o-",
                "example": true,
                "render": {
                    "scss": true
                }
            }
        }
    }
}
